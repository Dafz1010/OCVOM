<div class="home">
  <div class="logo">

    <%= link_to root_path do %>
      <%= image_tag 'logo 1.png' %>
      OCVOM
    <% end %>
  </div>
  <p style="color: green"><%= notice %></p>
  <br><br><br>
  <h1>Users</h1>

  <div id="users">
    <% @users.each do |user| %>
      <%= render user %>
      <p>
        <%= link_to "Show this user", user %>
      </p>
    <% end %>
  </div>

  <%= link_to "New user", new_user_path %>
</div>



