<div class="login">
	<%= render "home/logo" %>
	<div class="login-c">
		<%= form_with(url: session_path,local: true) do |f| %>
			<%= f.label :username, "Username" %>
			<%= f.text_field :username, autofocus: true %>
			<%= f.label :password, "Password" %>
			<%= f.password_field :password %>
			<%= f.submit "Sign In" %>
		<% end %>
	</div>	
</div>
